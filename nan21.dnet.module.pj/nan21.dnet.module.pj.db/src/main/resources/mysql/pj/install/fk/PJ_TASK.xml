	</changeSet>	
	<changeSet author="amathe" id="FK-3">
		<addForeignKeyConstraint constraintName="FK_PJ_TASK3" 
				baseTableName="PJ_TASK" baseColumnNames="STATUS_ID"  
				referencedTableName="PJ_TASK_STS"  referencedColumnNames="ID" />
	</changeSet>	
	<changeSet author="amathe" id="FK-4">
		<addForeignKeyConstraint constraintName="FK_PJ_TASK4" 
				baseTableName="PJ_TASK" baseColumnNames="ASSIGNEE_ID"  
				referencedTableName="PJ_PRJ_MBR"  referencedColumnNames="ID" />
	</changeSet>	
</databaseChangeLog>    
