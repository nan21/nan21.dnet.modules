<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:osgi-compendium="http://www.springframework.org/schema/osgi-compendium"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.springframework.org/schema/context 
				http://www.springframework.org/schema/context/spring-context.xsd
			http://www.springframework.org/schema/osgi-compendium
    			http://www.springframework.org/schema/osgi-compendium/spring-osgi-compendium.xsd">
	<context:annotation-config />
	<bean id="initDataProviderFactory" class="net.nan21.dnet.core.api.setup.InitDataProviderFactory">
		<property name="name" value="nan21.dnet.module.pj.data"></property>
	</bean>
	 
	<bean id="initDataProvider" scope="prototype"
		class="net.nan21.dnet.core.api.setup.InitDataProvider">
		<property name="list">
			<list>
				<bean class="net.nan21.dnet.core.api.setup.InitData">
					<property name="sequence" value="710" />
					<property name="name" value="pj-base" />
					<property name="items">
						<list>
							 
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="10" />
								<property name="dsName" value="IssueCategory" />
								<property name="destPath" value="demo-data/pj" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/pj/data/base/IssueCategory.csv" />
							</bean> 
							 
							
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="20" />
								<property name="dsName" value="IssueType" />
								<property name="destPath" value="demo-data/pj" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/pj/data/base/IssueType.csv" />
							</bean>
							
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="30" />
								<property name="dsName" value="IssueStatus" />
								<property name="destPath" value="demo-data/pj" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/pj/data/base/IssueStatus.csv" />
							</bean>
							 
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="40" />
								<property name="dsName" value="IssuePriority" />
								<property name="destPath" value="demo-data/pj" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/pj/data/base/IssuePriority.csv" />
							</bean>
							
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="50" />
								<property name="dsName" value="IssueSeverity" />
								<property name="destPath" value="demo-data/pj" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/pj/data/base/IssueSeverity.csv" />
							</bean>
							
							
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="60" />
								<property name="dsName" value="IssueResolution" />
								<property name="destPath" value="demo-data/pj" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/pj/data/base/IssueResolution.csv" />
							</bean>
							
							<!--  Project -->
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="70" />
								<property name="dsName" value="ProjectCategory" />
								<property name="destPath" value="demo-data/pj" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/pj/data/base/ProjectCategory.csv" />
							</bean>
							 
							 <bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="80" />
								<property name="dsName" value="ProjectType" />
								<property name="destPath" value="demo-data/pj" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/pj/data/base/ProjectType.csv" />
							</bean>
							 
							 <bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="90" />
								<property name="dsName" value="ProjectRole" />
								<property name="destPath" value="demo-data/pj" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/pj/data/base/ProjectRole.csv" />
							</bean>
							 
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="100" />
								<property name="dsName" value="IssueLinkType" />
								<property name="destPath" value="demo-data/pj" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/pj/data/base/IssueLinkType.csv" />
							</bean>
							
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="110" />
								<property name="dsName" value="IssueTaskType" />
								<property name="destPath" value="demo-data/pj" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/pj/data/base/IssueTaskType.csv" />
							</bean> 
							
							
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="120" />
								<property name="dsName" value="IssueTaskStatus" />
								<property name="destPath" value="demo-data/pj" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/pj/data/base/IssueTaskStatus.csv" />
							</bean> 
							 
						</list>
					</property>
				</bean>
				
				
				
				
				<bean class="net.nan21.dnet.core.api.setup.InitData">
					<property name="sequence" value="720" />
					<property name="name" value="pj-md" />
					<property name="items">
						<list>
							 
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="10" />
								<property name="dsName" value="Project" />
								<property name="destPath" value="demo-data/pj" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/pj/data/md/Project.csv" />
							</bean> 
							
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="20" />
								<property name="dsName" value="ProjectVersion" />
								<property name="destPath" value="demo-data/pj" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/pj/data/md/ProjectVersion.csv" />
							</bean> 
							    
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="30" />
								<property name="dsName" value="ProjectComponent" />
								<property name="destPath" value="demo-data/pj" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/pj/data/md/ProjectComponent.csv" />
							</bean>  
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="40" />
								<property name="dsName" value="ProjectMember" />
								<property name="destPath" value="demo-data/pj" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/pj/data/md/ProjectMember.csv" />
							</bean>  
						</list>
					</property>
				</bean>
				 
			</list>
		</property>
	</bean>
</beans>