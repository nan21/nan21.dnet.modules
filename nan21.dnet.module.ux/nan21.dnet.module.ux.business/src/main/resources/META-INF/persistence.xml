<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
	version="1.0">
	<persistence-unit name="default" transaction-type="RESOURCE_LOCAL">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		
		<class>net.nan21.dnet.module.ux.example.domain.entity.LandFormType</class>
		<class>net.nan21.dnet.module.ux.example.domain.entity.LandForm</class>

		<!-- Dependencies -->
		<class>net.nan21.dnet.module.bd.geo.domain.entity.Country</class>
		<properties>
			<property name="eclipselink.weaving" value="none" />
		</properties>
	</persistence-unit>
</persistence>