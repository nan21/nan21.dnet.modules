<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
    <changeSet author="amathe" id="TBL">
        <createTable tableName="BP_CLNDR_EVENT_STATUS">
			<column name="ACTIVE" type="number(1)">
				<constraints nullable="false" /></column>	 
			<column name="CLIENTID" type="number(3)">
				<constraints nullable="false" /></column>	 
			<column name="CREATEDAT" type="date">
				<constraints nullable="false" /></column>	 
			<column name="CREATEDBY" type="varchar2(32)">
				<constraints nullable="false" /></column>	 
			<column name="DESCRIPTION" type="varchar2(400)">
				<constraints  /></column>	 
			<column name="EVENTTYPE" type="varchar2(16)">
				<constraints  /></column>	 
			<column name="ID" type="number(19)">
				<constraints nullable="false" /></column>	 
			<column name="MODIFIEDAT" type="date">
				<constraints nullable="false" /></column>	 
			<column name="MODIFIEDBY" type="varchar2(32)">
				<constraints nullable="false" /></column>	 
			<column name="NAME" type="varchar2(255)">
				<constraints nullable="false" /></column>	 
			<column name="UUID" type="varchar2(36)">
				<constraints  /></column>	 
			<column name="VERSION" type="number(19)">
				<constraints nullable="false" /></column>	 
        </createTable>
    </changeSet>    
    <changeSet author="amathe" id="PK">
    	<addPrimaryKey tableName="BP_CLNDR_EVENT_STATUS" constraintName="PK_CLNDR_EVENT_STATUS" columnNames="ID" />
    </changeSet>       
	<changeSet author="amathe" id="UK-1">
		<addUniqueConstraint tableName="BP_CLNDR_EVENT_STATUS" constraintName="UK_CLNDR_EVENT_STATUS1" columnNames="CLIENTID, EVENTTYPE,NAME" />
	</changeSet>
	<changeSet author="amathe" id="SEQ">
		<insert tableName="SEQUENCE">
		    <column name="SEQ_NAME" value="BP_CLNDR_EVENT_STATUS_SEQ"/>
		</insert>
	</changeSet>
</databaseChangeLog>
