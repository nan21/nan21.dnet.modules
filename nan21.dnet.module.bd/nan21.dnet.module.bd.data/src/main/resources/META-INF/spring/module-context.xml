<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:osgi-compendium="http://www.springframework.org/schema/osgi-compendium"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.springframework.org/schema/context 
				http://www.springframework.org/schema/context/spring-context.xsd
			http://www.springframework.org/schema/osgi-compendium
    			http://www.springframework.org/schema/osgi-compendium/spring-osgi-compendium.xsd">
	<context:annotation-config />
	<bean id="initDataProviderFactory" class="net.nan21.dnet.core.api.setup.InitDataProviderFactory">
		<property name="name" value="nan21.dnet.module.bd.data"></property>
	</bean>
	<bean id="initDataProvider" scope="prototype"
		class="net.nan21.dnet.core.api.setup.InitDataProvider">
		<property name="list">
			<list>
				<!-- Currencies -->
				<bean class="net.nan21.dnet.core.api.setup.InitData">
					<property name="sequence" value="110" />
					<property name="name" value="bd-currency" />
					<property name="items">
						<list>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="10" />
								<property name="dsName" value="Currency" />
								<property name="destPath" value="demo-data/bd" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/bd/data/currency/Currency.csv" />
							</bean>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="20" />
								<property name="dsName" value="CurrencyXRateProvider" />
								<property name="destPath" value="demo-data/bd" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/bd/data/currency/CurrencyXRateProvider.csv" />
							</bean>
						</list>
					</property>
				</bean>
				<!-- Measuring units -->
				<bean class="net.nan21.dnet.core.api.setup.InitData">
					<property name="sequence" value="120" />
					<property name="name" value="bd-uom" />
					<property name="items">
						<list>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="10" />
								<property name="dsName" value="UomType" />
								<property name="destPath" value="demo-data/bd" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/bd/data/uom/UomType.csv" />
							</bean>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="20" />
								<property name="dsName" value="Uom" />
								<property name="destPath" value="demo-data/bd" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/bd/data/uom/Uom.csv" />
							</bean>
						</list>
					</property>
				</bean>
				<!-- Geography -->
				<bean class="net.nan21.dnet.core.api.setup.InitData">
					<property name="sequence" value="130" />
					<property name="name" value="bd-geo" />
					<property name="items">
						<list>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="10" />
								<property name="dsName" value="Country" />
								<property name="destPath" value="demo-data/bd" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/bd/data/geo/Country.csv" />
							</bean>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="20" />
								<property name="dsName" value="Region" />
								<property name="destPath" value="demo-data/bd" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/bd/data/geo/Region.csv" />
							</bean>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="50" />
								<property name="dsName" value="Location" />
								<property name="destPath" value="demo-data/bd" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/bd/data/geo/Location.csv" />
							</bean>
						</list>
					</property>
				</bean>
				<!-- Contact -->
				<bean class="net.nan21.dnet.core.api.setup.InitData">
					<property name="sequence" value="140" />
					<property name="name" value="bd-contact" />
					<property name="items">
						<list>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="10" />
								<property name="dsName" value="CommunicationChannelType" />
								<property name="destPath" value="demo-data/bd" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/bd/data/contact/CommunicationChannelType.csv" />
							</bean>
						</list>
					</property>
				</bean>
				<!-- Organization -->
				<bean class="net.nan21.dnet.core.api.setup.InitData">
					<property name="sequence" value="150" />
					<property name="name" value="bd-org" />
					<property name="items">
						<list>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="10" />
								<property name="dsName" value="OrganizationType" />
								<property name="destPath" value="demo-data/bd" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/bd/data/org/OrganizationType.csv" />
							</bean>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="20" />
								<property name="dsName" value="Organization" />
								<property name="destPath" value="demo-data/bd" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/bd/data/org/Organization.csv" />
							</bean>
						</list>
					</property>
				</bean>
				<!-- Classification standards -->
				<bean class="net.nan21.dnet.core.api.setup.InitData">
					<property name="sequence" value="160" />
					<property name="name" value="bd-standards" />
					<property name="items">
						<list>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="10" />
								<property name="dsName" value="ClassificationSystem" />
								<property name="destPath" value="demo-data/bd" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/bd/data/standards/ClassificationSystem.csv" />
							</bean>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="20" />
								<property name="dsName" value="ClassificationItem" />
								<property name="destPath" value="demo-data/bd" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/bd/data/standards/ClassificationItem.csv" />
							</bean>
						</list>
					</property>
				</bean>
				<!-- Financial base data -->
				<bean class="net.nan21.dnet.core.api.setup.InitData">
					<property name="sequence" value="170" />
					<property name="name" value="bd-fin" />
					<property name="items">
						<list>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="10" />
								<property name="dsName" value="TaxCategory" />
								<property name="destPath" value="demo-data/bd" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/bd/data/fin/TaxCategory.csv" />
							</bean>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="20" />
								<property name="dsName" value="Tax" />
								<property name="destPath" value="demo-data/bd" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/bd/data/fin/Tax.csv" />
							</bean>
						</list>
					</property>
				</bean>
				<!-- Transactions base data -->
				<bean class="net.nan21.dnet.core.api.setup.InitData">
					<property name="sequence" value="180" />
					<property name="name" value="bd-tx" />
					<property name="items">
						<list>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="10" />
								<property name="dsName" value="TxDocType" />
								<property name="destPath" value="demo-data/bd" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/bd/data/tx/TxDocType.csv" />
							</bean>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="30" />
								<property name="dsName" value="DeliveryMethod" />
								<property name="destPath" value="demo-data/bd" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/bd/data/tx/DeliveryMethod.csv" />
							</bean>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="50" />
								<property name="dsName" value="PaymentMethod" />
								<property name="destPath" value="demo-data/bd" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/bd/data/tx/PaymentMethod.csv" />
							</bean>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="60" />
								<property name="dsName" value="PaymentTerm" />
								<property name="destPath" value="demo-data/bd" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/bd/data/tx/PaymentTerm.csv" />
							</bean>
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>
</beans>