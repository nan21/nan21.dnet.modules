	  	<property name="modelClass" value="net.nan21.dnet.module.bd.uom.ds.model.UomConversionDs" />
	  	<property name="entityClass" value="net.nan21.dnet.module.bd.uom.domain.entity.UomConversion" />
	  	<property name="converterClass" value="net.nan21.dnet.module.bd.uom.ds.converter.UomConversionDsConv" />
