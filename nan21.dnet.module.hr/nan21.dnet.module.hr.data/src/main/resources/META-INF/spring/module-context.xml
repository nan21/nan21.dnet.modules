<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:osgi-compendium="http://www.springframework.org/schema/osgi-compendium"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.springframework.org/schema/context 
				http://www.springframework.org/schema/context/spring-context.xsd
			http://www.springframework.org/schema/osgi-compendium
    			http://www.springframework.org/schema/osgi-compendium/spring-osgi-compendium.xsd">
	<context:annotation-config />
	<bean id="initDataProviderFactory" class="net.nan21.dnet.core.api.setup.InitDataProviderFactory">
		<property name="name" value="nan21.dnet.module.hr.data"></property>
	</bean>
	
	<bean id="initDataProvider" scope="prototype"
		class="net.nan21.dnet.core.api.setup.InitDataProvider">
		<property name="list">
			<list>
			  
				<bean class="net.nan21.dnet.core.api.setup.InitData">
					<property name="sequence" value="10" />
					<property name="name" value="hr-time" />
					<property name="items">
						<list>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="10" />
								<property name="dsName" value="AbsenceCategory" />
								<property name="destPath" value="demo-data/hr/time" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/hr/data/time/AbsenceCategory.csv" />
							</bean>
							
							
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="20" />
								<property name="dsName" value="AbsenceType" />
								<property name="destPath" value="demo-data/hr/time" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/hr/data/time/AbsenceType.csv" />
							</bean>
							
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="30" />
								<property name="dsName" value="AbsenceReason" />
								<property name="destPath" value="demo-data/hr/time" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/hr/data/time/AbsenceReason.csv" />
							</bean>
							
							 
						</list>
					</property>
				</bean>
				 
				 
				 
				<bean class="net.nan21.dnet.core.api.setup.InitData">
					<property name="sequence" value="20" />
					<property name="name" value="hr-skill" />
					<property name="items">
						<list>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="10" />
								<property name="dsName" value="RatingScale" />
								<property name="destPath" value="demo-data/hr/skill" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/hr/data/skill/RatingScale.csv" />
							</bean>
							
							 
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="10" />
								<property name="dsName" value="RatingLevel" />
								<property name="destPath" value="demo-data/hr/skill" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/hr/data/skill/RatingLevel.csv" />
							</bean>
							 
						</list>
					</property>
				</bean> 
				
				
				<bean class="net.nan21.dnet.core.api.setup.InitData">
					<property name="sequence" value="30" />
					<property name="name" value="hr-training" />
					<property name="items">
						<list>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="10" />
								<property name="dsName" value="CourseCategory" />
								<property name="destPath" value="demo-data/hr/training" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/hr/data/training/CourseCategory.csv" />
							</bean>
							
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="20" />
								<property name="dsName" value="CourseType" />
								<property name="destPath" value="demo-data/hr/training" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/hr/data/training/CourseType.csv" />
							</bean>
							 
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="30" />
								<property name="dsName" value="Course" />
								<property name="destPath" value="demo-data/hr/training" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/hr/data/training/Course.csv" />
							</bean> 
						</list>
					</property>
				</bean> 
				
			</list>
		</property>
	</bean>
</beans>