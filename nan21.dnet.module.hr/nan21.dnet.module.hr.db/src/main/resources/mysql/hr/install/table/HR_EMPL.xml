<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
    <changeSet author="amathe" id="TBL">
        <createTable tableName="HR_EMPL">
	
    		<column name="ID" type="bigint(19)" />
			<column name="ASSIGNTOPOSITION" type="int(1)">
				<constraints nullable="false" /></column>	 
			<column name="BASESALARY" type="decimal(12,2)">
				<constraints  /></column>	 
			<column name="GRADE_ID" type="bigint(19)">	 
				<constraints  /></column>	 
			<column name="JOB_ID" type="bigint(19)">	 
				<constraints  /></column>	 
			<column name="ORG_ID" type="bigint(19)">	 
				<constraints  /></column>	 
			<column name="PAYROLL_ID" type="bigint(19)">	 
				<constraints  /></column>	 
			<column name="POSITION_ID" type="bigint(19)">	 
				<constraints  /></column>	 
			<column name="TYPE_ID" type="bigint(19)">	 
				<constraints nullable="false" /></column>	 
        </createTable>
    </changeSet>    
    <changeSet author="amathe" id="PK">
    	<addPrimaryKey tableName="HR_EMPL" constraintName="PK_EMPL" columnNames="ID" />
    </changeSet>       
	<changeSet author="amathe" id="SEQ">
		<insert tableName="SEQUENCE">
		    <column name="SEQ_NAME" value="HR_EMPL_SEQ"/>
		    <column name="SEQ_COUNT" valueNumeric="0"/>
		</insert>
	</changeSet>
</databaseChangeLog>
