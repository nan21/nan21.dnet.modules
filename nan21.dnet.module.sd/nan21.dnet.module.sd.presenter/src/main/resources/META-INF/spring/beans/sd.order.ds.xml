		<property name="modelClass" value="net.nan21.dnet.module.sd.order.ds.model.SalesOrderItemDs" />
	  	<property name="serviceMethods" >
	  		<list>
	  			<value type="java.lang.String">onProductChange</value>
			</list>	  		 
	  	</property>	
	</bean>
   
