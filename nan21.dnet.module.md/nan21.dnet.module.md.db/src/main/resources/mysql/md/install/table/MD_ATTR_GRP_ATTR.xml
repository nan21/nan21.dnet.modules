<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
    <changeSet author="amathe" id="TBL">
        <createTable tableName="MD_ATTR_GRP_ATTR">
			<column name="CLIENTID" type="bigint(3)">
				<constraints nullable="false" /></column>	 
			<column name="CREATEDAT" type="datetime">
				<constraints nullable="false" /></column>	 
			<column name="CREATEDBY" type="varchar(32)">
				<constraints nullable="false" /></column>	 
			<column name="ID" type="bigint(19)">
				<constraints nullable="false" /></column>	 
			<column name="INDESCRIPTION" type="int(1)">
				<constraints nullable="false" /></column>	 
			<column name="MODIFIEDAT" type="datetime">
				<constraints nullable="false" /></column>	 
			<column name="MODIFIEDBY" type="varchar(32)">
				<constraints nullable="false" /></column>	 
			<column name="SEQUENCENO" type="int(10)">
				<constraints  /></column>	 
			<column name="UUID" type="varchar(36)">
				<constraints  /></column>	 
			<column name="VERSION" type="bigint(19)">
				<constraints nullable="false" /></column>	 
			<column name="ATTRIBUTE_ID" type="bigint(19)">	 
				<constraints nullable="false" /></column>	 
			<column name="GROUP_ID" type="bigint(19)">	 
				<constraints nullable="false" /></column>	 
        </createTable>
    </changeSet>    
    <changeSet author="amathe" id="PK">
    	<addPrimaryKey tableName="MD_ATTR_GRP_ATTR" constraintName="PK_ATTR_GRP_ATTR" columnNames="ID" />
    </changeSet>       
	<changeSet author="amathe" id="UK-1">
		<addUniqueConstraint tableName="MD_ATTR_GRP_ATTR" constraintName="UK_ATTR_GRP_ATTR1" columnNames="CLIENTID, GROUP_ID,ATTRIBUTE_ID" />
	</changeSet>
	<changeSet author="amathe" id="SEQ">
		<insert tableName="SEQUENCE">
		    <column name="SEQ_NAME" value="MD_ATTR_GRP_ATTR_SEQ"/>
		    <column name="SEQ_COUNT" valueNumeric="0"/>
		</insert>
	</changeSet>
</databaseChangeLog>
