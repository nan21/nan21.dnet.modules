<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
    <changeSet author="amathe" id="TBL">
        <createTable tableName="MD_BP_CONTACT">
			<column name="ACTIVE" type="int(1)">
				<constraints nullable="false" /></column>	 
			<column name="BIRTHDATE" type="date">
				<constraints  /></column>	 
			<column name="CLIENTID" type="bigint(3)">
				<constraints nullable="false" /></column>	 
			<column name="CREATEDAT" type="datetime">
				<constraints nullable="false" /></column>	 
			<column name="CREATEDBY" type="varchar(32)">
				<constraints nullable="false" /></column>	 
			<column name="ENTITYTYPE" type="varchar(32)">
				<constraints  /></column>	 
			<column name="FIRSTNAME" type="varchar(255)">
				<constraints  /></column>	 
			<column name="GENDER" type="varchar(16)">
				<constraints  /></column>	 
			<column name="ID" type="bigint(19)">
				<constraints nullable="false" /></column>	 
			<column name="LASTNAME" type="varchar(255)">
				<constraints  /></column>	 
			<column name="MIDDLENAME" type="varchar(255)">
				<constraints  /></column>	 
			<column name="MODIFIEDAT" type="datetime">
				<constraints nullable="false" /></column>	 
			<column name="MODIFIEDBY" type="varchar(32)">
				<constraints nullable="false" /></column>	 
			<column name="POSITION" type="varchar(255)">
				<constraints  /></column>	 
			<column name="UUID" type="varchar(36)">
				<constraints  /></column>	 
			<column name="VERSION" type="bigint(19)">
				<constraints nullable="false" /></column>	 
			<column name="BPARTNER_ID" type="bigint(19)">	 
				<constraints  /></column>	 
        </createTable>
    </changeSet>    
    <changeSet author="amathe" id="PK">
    	<addPrimaryKey tableName="MD_BP_CONTACT" constraintName="PK_BP_CONTACT" columnNames="ID" />
    </changeSet>       
	<changeSet author="amathe" id="SEQ">
		<insert tableName="SEQUENCE">
		    <column name="SEQ_NAME" value="MD_BP_CONTACT_SEQ"/>
		    <column name="SEQ_COUNT" valueNumeric="0"/>
		</insert>
	</changeSet>
</databaseChangeLog>
