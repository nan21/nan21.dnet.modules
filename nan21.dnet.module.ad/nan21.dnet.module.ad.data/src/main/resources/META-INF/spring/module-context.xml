<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:osgi-compendium="http://www.springframework.org/schema/osgi-compendium"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.springframework.org/schema/context 
				http://www.springframework.org/schema/context/spring-context.xsd
			http://www.springframework.org/schema/osgi-compendium
    			http://www.springframework.org/schema/osgi-compendium/spring-osgi-compendium.xsd">
	<context:annotation-config />
	<bean id="initDataProviderFactory" class="net.nan21.dnet.core.api.setup.InitDataProviderFactory">
		<property name="name" value="nan21.dnet.module.ad.data"></property>
	</bean>
	
	<bean id="initDataProvider" scope="prototype"
		class="net.nan21.dnet.core.api.setup.InitDataProvider">
		<property name="list">
			<list>
			
				<bean class="net.nan21.dnet.core.api.setup.InitData">
					<property name="sequence" value="5" />
					<property name="name" value="ad-system" />
					<property name="items">
						<list>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="10" />
								<property name="dsName" value="SysDateFormat" />
								<property name="destPath" value="demo-data/ad" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/ad/data/system/SysDateFormat.csv" />
							</bean>	
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="20" />
								<property name="dsName" value="SysParam" />
								<property name="destPath" value="demo-data/ad" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/ad/data/system/SysParam.csv" />
							</bean>						   
						</list>
					</property>
				</bean>
				
				
				<bean class="net.nan21.dnet.core.api.setup.InitData">
					<property name="sequence" value="10" />
					<property name="name" value="ad-usr" />
					<property name="items">
						<list>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="10" />
								<property name="dsName" value="UserGroup" />
								<property name="destPath" value="demo-data/ad" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/ad/data/usr/UserGroup.csv" />
							</bean>	
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="20" />
								<property name="dsName" value="AssignableType" />
								<property name="destPath" value="demo-data/ad" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/ad/data/usr/AssignableType.csv" />
							</bean>	
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="30" />
								<property name="dsName" value="Assignable" />
								<property name="destPath" value="demo-data/ad" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/ad/data/usr/Assignable.csv" />
							</bean>		
							
							
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="50" />
								<property name="dsName" value="Menu" />
								<property name="destPath" value="demo-data/ad" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/ad/data/usr/Menu.csv" />
							</bean>	
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="60" />
								<property name="dsName" value="MenuItem" />
								<property name="destPath" value="demo-data/ad" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/ad/data/usr/MenuItem.csv" />
							</bean>		
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="70" />
								<property name="dsName" value="MenuItem" />
								<property name="destPath" value="demo-data/ad" />
								<property name="ukFieldName" value="name" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/ad/data/usr/MenuItemParent.csv" />
							</bean>		   
						</list>
					</property>
				</bean>
				
				<bean class="net.nan21.dnet.core.api.setup.InitData">
					<property name="sequence" value="20" />
					<property name="name" value="ad-data" />
					<property name="items">
						<list>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="10" />
								<property name="dsName" value="AttachmentType" />
								<property name="destPath" value="demo-data/ad" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/ad/data/data/AttachmentType.csv" />
							</bean>						   
						</list>
					</property>
				</bean>
				
				
				<bean class="net.nan21.dnet.core.api.setup.InitData">
					<property name="sequence" value="40" />
					<property name="name" value="ad-report" />
					<property name="items">
						<list>
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="10" />
								<property name="dsName" value="ReportServer" />
								<property name="destPath" value="demo-data/ad" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/ad/data/report/ReportServer.csv" />
							</bean>
						  
						  	<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="20" />
								<property name="dsName" value="Report" />
								<property name="destPath" value="demo-data/ad" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/ad/data/report/Report.csv" />
							</bean>
							
							<bean class="net.nan21.dnet.core.api.setup.InitDataItem">
								<property name="sequence" value="30" />
								<property name="dsName" value="ReportParam" />
								<property name="destPath" value="demo-data/ad" />
								<property name="file"
									value="classpath:net/nan21/dnet/module/ad/data/report/ReportParam.csv" />
							</bean>
							
						</list>
					</property>
				</bean>
			 
			</list>
		</property>
	</bean>
</beans>