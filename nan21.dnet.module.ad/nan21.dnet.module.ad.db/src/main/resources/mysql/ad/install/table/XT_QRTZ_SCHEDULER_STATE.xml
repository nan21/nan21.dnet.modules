<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
    <changeSet author="amathe" id="TBL">
        <createTable tableName="XT_QRTZ_SCHEDULER_STATE">
			<column name="CHECKIN_INTERVAL" type="int(8)">
				<constraints  /></column>	 
			<column name="INSTANCE_NAME" type="varchar(255)">
				<constraints nullable="false" /></column>	 
			<column name="LAST_CHECKIN_TIME" type="datetime">
				<constraints  /></column>	 
			<column name="SCHED_NAME" type="varchar(255)">
				<constraints nullable="false" /></column>	 
        </createTable>
    </changeSet>    
    <changeSet author="amathe" id="PK">
    	<addPrimaryKey tableName="XT_QRTZ_SCHEDULER_STATE" constraintName="PK_QRTZ_SCHEDULER_STATE" columnNames="SCHED_NAME,INSTANCE_NAME" />
    </changeSet>       
	<changeSet author="amathe" id="SEQ">
		<insert tableName="SEQUENCE">
		    <column name="SEQ_NAME" value="XT_QRTZ_SCHEDULER_STATE_SEQ"/>
		    <column name="SEQ_COUNT" valueNumeric="0"/>
		</insert>
	</changeSet>
</databaseChangeLog>
