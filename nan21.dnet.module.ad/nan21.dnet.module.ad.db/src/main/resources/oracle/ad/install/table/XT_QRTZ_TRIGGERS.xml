<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
    <changeSet author="amathe" id="TBL">
        <createTable tableName="XT_QRTZ_TRIGGERS">
			<column name="CALENDAR_NAME" type="varchar2(255)">
				<constraints  /></column>	 
			<column name="DESCRIPTION" type="varchar2(400)">
				<constraints  /></column>	 
			<column name="END_TIME" type="date">
				<constraints  /></column>	 
			<column name="JOB_DATA" type="">
				<constraints  /></column>	 
			<column name="JOB_GROUP" type="varchar2(255)">
				<constraints nullable="false" /></column>	 
			<column name="JOB_NAME" type="varchar2(255)">
				<constraints nullable="false" /></column>	 
			<column name="MISFIRE_INSTR" type="number(2)">
				<constraints  /></column>	 
			<column name="NEXT_FIRE_TIME" type="date">
				<constraints  /></column>	 
			<column name="PREV_FIRE_TIME" type="date">
				<constraints  /></column>	 
			<column name="PRIORITY" type="number(10)">
				<constraints  /></column>	 
			<column name="SCHED_NAME" type="varchar2(255)">
				<constraints nullable="false" /></column>	 
			<column name="START_TIME" type="date">
				<constraints  /></column>	 
			<column name="TRIGGER_GROUP" type="varchar2(255)">
				<constraints nullable="false" /></column>	 
			<column name="TRIGGER_NAME" type="varchar2(255)">
				<constraints nullable="false" /></column>	 
			<column name="TRIGGER_STATE" type="varchar2(32)">
				<constraints  /></column>	 
			<column name="TRIGGER_TYPE" type="varchar2(32)">
				<constraints  /></column>	 
        </createTable>
    </changeSet>    
    <changeSet author="amathe" id="PK">
    	<addPrimaryKey tableName="XT_QRTZ_TRIGGERS" constraintName="PK_QRTZ_TRIGGERS" columnNames="SCHED_NAME,TRIGGER_NAME,TRIGGER_GROUP" />
    </changeSet>       
	<changeSet author="amathe" id="SEQ">
		<insert tableName="SEQUENCE">
		    <column name="SEQ_NAME" value="XT_QRTZ_TRIGGERS_SEQ"/>
		    <column name="SEQ_COUNT" valueNumeric="0"/>
		</insert>
	</changeSet>
</databaseChangeLog>
