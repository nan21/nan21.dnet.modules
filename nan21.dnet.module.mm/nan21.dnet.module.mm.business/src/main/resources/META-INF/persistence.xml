<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
	version="1.0">
	<persistence-unit name="net.nan21.dnet"
		transaction-type="RESOURCE_LOCAL">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		
		<class>net.nan21.dnet.module.mm.md.domain.entity.ProductCategory</class>
		<class>net.nan21.dnet.module.mm.md.domain.entity.Product</class>
		<class>net.nan21.dnet.module.mm.md.domain.entity.ProductAttachment</class>
		<class>net.nan21.dnet.module.mm.md.domain.entity.ProductManufacturer</class>
		<class>net.nan21.dnet.module.mm.md.domain.entity.ProductSubstitute</class>
		<class>net.nan21.dnet.module.mm.md.domain.entity.ProductAccountGroup</class>
		<class>net.nan21.dnet.module.mm.md.domain.entity.ProductAccount</class>

		<!-- Dependencies -->
		<class>net.nan21.dnet.module.bd.currency.domain.entity.Currency</class>
		<class>net.nan21.dnet.module.bd.uom.domain.entity.Uom</class>
		<class>net.nan21.dnet.module.bd.uom.domain.entity.UomConversion</class>
		<class>net.nan21.dnet.module.ad.org.domain.entity.Organization</class>
		<class>net.nan21.dnet.module.ad.org.domain.entity.OrganizationType</class>
		<properties>
			<property name="eclipselink.weaving" value="none" />
		</properties>
	</persistence-unit>
</persistence>