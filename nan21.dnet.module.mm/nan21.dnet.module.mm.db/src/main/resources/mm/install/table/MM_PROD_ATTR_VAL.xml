	</changeSet>
	<changeSet author="amathe" id="SEQ">
		<insert tableName="SEQUENCE">
		    <column name="SEQ_NAME" value="MM_PROD_ATTR_VAL_SEQ"/>
		</insert>
	</changeSet>
</databaseChangeLog>
